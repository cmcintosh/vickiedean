<div class="box blog-search">
	<div class="box-heading"><h3>Search Article</h3></div>
	<div class="box-content">
		<div id="blog-search">
			<div>
				<input type="text" name="article_search" value="" placeholder="Search Article" class="form-control" style="margin-bottom: 5px;"/>
				<a id="button-search" href="#"></a>
			</div>
		</div>
	</div>
</div>
<div class="container">
	<div class="box blog_articles">
			<div class="box-heading">
				<h3>Popular Articles</h3>
			</div>
		<div class="box-content">
			<div class="row">
				{% for row in rows %}
					{%
						set row_classes = [
							default_row_class ? 'views-row',
						]
					%}
				{{ row.content }}
				{% endfor %}
			</div>
		</div>
	</div>
</div>