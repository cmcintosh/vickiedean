<div class="box featured">
	<div class="box-heading">
		<h3>Latest</h3>
	</div>
	<div class="box-content">
		<div class="row">
			<div class="product-layout col-lg-3 col-md-3 col-sm-3 col-xs-12">
				
						{% for row in rows %}
							{%
								set row_classes = [
									default_row_class ? 'views-row',
								]
							%}
						{{ row.content }} 
						{% endfor %}

			</div> 
		</div>
	</div>
</div> 