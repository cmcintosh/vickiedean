use Drupal\Core\Routing\RouteMatchInterface;


/**
* Implements hook_theme to allow custom page.
*/
function order_forms_theme($existing, $type, $theme, $path) {

  return array(
    'user_account' => array(
      'render element' => 'form',
      'template' => 'user_account',
      'path' => $path . '/templates'
    ),
  );
}

 